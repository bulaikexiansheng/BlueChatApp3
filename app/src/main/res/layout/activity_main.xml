<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/app_bg2"
    tools:context=".MainActivity.MainActivity">

    <com.nightonke.boommenu.BoomMenuButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            app:bmb_buttonEnum="textInsideCircle"
            app:bmb_piecePlaceEnum="piecePlace_dot_2_1"
            app:bmb_buttonPlaceEnum="buttonPlace_sc_2_1"
            android:id="@+id/boomMenu_Button"
            app:bmb_boomInWholeScreen="false"
          >
    </com.nightonke.boommenu.BoomMenuButton>
    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/boomMenu_Button"
        android:layout_marginTop="16dp"
        android:id="@+id/toast_control"
        android:text="toast:on"
        >

    </com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="200dp"
        android:layout_marginTop="50dp"
        android:layout_gravity="center_horizontal"
        android:id="@id/icon"
        android:src="@drawable/logo5" />
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/icon"
        android:id="@+id/chart_fragment_layout"></FrameLayout>
</RelativeLayout>